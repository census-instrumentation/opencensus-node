{
  "name": "@opencensus/exporter-ocagent",
  "version": "0.0.15",
  "description": "OpenCensus Agent Exporter allows user to send collected trace to the OpenCensus Agent",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "census-instrumentation/opencensus-node",
  "scripts": {
    "test": "nyc mocha build/test/**/*.js",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "rimraf build/*",
    "check": "gts check",
    "compile": "tsc -p .",
    "postcompile": "npm run protos:copy",
    "watch": "nodemon --exec tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "protos:copy": "cp -R src/protos build/src/protos && cp -R node_modules/google-proto-files/google build/src/protos/google"
  },
  "keywords": [
    "opencensus",
    "nodejs",
    "tracing",
    "profiling"
  ],
  "author": "OpenCensus Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "build/src/**/*.proto",
    "doc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ],
    "ext": "ts"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/uuid": "^3.4.3",
    "codecov": "^3.5.0",
    "google-proto-files": "^1.0.0",
    "gts": "^1.0.0",
    "mocha": "^6.1.0",
    "nodemon": "^1.18.7",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^8.0.0",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.0",
    "@opencensus/core": "^0.0.15",
    "@opencensus/nodejs": "^0.0.15",
    "grpc": "^1.20.3"
  }
}
