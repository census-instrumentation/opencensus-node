{
	"name": "@opencensus/exporter-object",
	"version": "0.0.22",
	"description": "OpenCensus Object Exporter allows the user to collect and access traces with OpenCensus Node.js.",
	"main": "build/src/index.js",
	"types": "build/src/index.d.ts",
	"repository": "census-instrumentation/opencensus-node",
	"scripts": {
		"test": "nyc ts-mocha -p ./tsconfig.json test/**/*.ts",
		"codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
		"clean": "rimraf build/*",
		"check": "gts check",
		"compile": "tsc -p .",
		"fix": "gts fix",
		"prepare": "npm run compile",
		"posttest": "npm run check"
	},
	"keywords": [
		"opencensus",
		"nodejs",
		"tracing",
		"profiling"
	],
	"author": "Google Inc.",
	"license": "Apache-2.0",
	"engines": {
		"node": ">=8"
	},
	"files": [
		"build/src/**/*.js",
		"build/src/**/*.d.ts",
		"doc",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts",
			"build/**/**/*.js"
		],
		"all": true
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/mocha": "^7.0.0",
		"@types/nock": "^10.0.0",
		"@types/node": "^10.12.12",
		"codecov": "^3.5.0",
		"gts": "^1.0.0",
		"mocha": "^7.0.0",
		"nyc": "14.1.1",
		"ts-mocha": "^7.0.0",
		"ts-node": "^9.0.0",
		"typescript": "~3.2.0"
	},
	"dependencies": {
		"@opencensus/core": "^0.0.22"
	}
}
