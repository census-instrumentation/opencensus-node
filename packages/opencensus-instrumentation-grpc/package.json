{
  "name": "@opencensus/instrumentation-grpc",
  "version": "0.1.0",
  "description": "Opencensus grpc automatic instrumentation package.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "census-instrumentation/opencensus-node",
  "scripts": {
    "test": "nyc ts-mocha -p ./tsconfig.json test/**/*.ts",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "rimraf build/*",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run check",
    "copy-fixtures": "cp -r ./test/fixtures ./build/test/"
  },
  "keywords": [
    "opencensus",
    "nodejs",
    "tracing",
    "profiling"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "build/**/**/*.js"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/lodash": "4.14.171",
    "@types/mocha": "8.2.3",
    "@types/node": "14.17.27",
    "@types/shimmer": "1.0.2",
    "codecov": "3.8.3",
    "gts": "1.1.2",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "protobufjs": "6.11.2",
    "rimraf": "3.0.2",
    "ts-mocha": "8.0.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.10"
  },
  "dependencies": {
    "@opencensus/core": "^0.1.0",
    "@opencensus/propagation-binaryformat": "^0.1.0",
    "grpc": "^1.24.10",
    "lodash": "^4.17.11",
    "object-sizeof": "^1.3.0",
    "shimmer": "^1.2.0"
  }
}
