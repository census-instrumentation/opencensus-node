{
  "name": "@opencensus/instrumentation-grpc",
  "version": "0.0.8",
  "description": "Opencensus grpc automatic instrumentation package.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "census-instrumentation/opencensus-node",
  "scripts": {
    "test": "nyc mocha build/test/**/*.js",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "rimraf build/*",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile; npm run copy-fixtures",
    "posttest": "npm run check",
    "copy-fixtures": "cp -r ./test/fixtures ./build/test/"
  },
  "keywords": [
    "opencensus",
    "nodejs",
    "tracing",
    "profiling"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@opencensus/propagation-b3": "^0.0.8",
    "@types/end-of-stream": "^1.4.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.1.3",
    "@types/node": "^10.12.12",
    "@types/protobufjs": "^6.0.0",
    "@types/semver": "^5.5.0",
    "@types/shimmer": "^1.0.1",
    "@types/uuid": "^3.4.3",
    "codecov": "^3.1.0",
    "gts": "^0.9.0",
    "mocha": "^5.0.4",
    "ncp": "^2.0.0",
    "nock": "^10.0.0",
    "nyc": "^13.0.0",
    "protobufjs": "^6.8.6",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "@opencensus/core": "^0.0.8",
    "grpc": "~1.12.2",
    "lodash": "^4.17.10",
    "semver": "^5.5.0",
    "shimmer": "^1.2.0"
  }
}
