{
  "name": "@opencensus/propagation-jaeger",
  "version": "0.0.14",
  "description": "Opencensus propagation package for jaeger's format.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "census-instrumentation/opencensus-node",
  "scripts": {
    "test": "nyc ts-mocha -p ./tsconfig.json test/**/*.ts",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "rimraf build/*",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "opencensus",
    "nodejs",
    "tracing",
    "profiling",
    "jaeger"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "build/src",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "build/**/**/*.js"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@types/uuid": "^3.4.3",
    "codecov": "^3.4.0",
    "gts": "^0.9.0",
    "mocha": "^6.1.0",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.0.0",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "@opencensus/core": "^0.0.14",
    "uuid": "^3.2.1"
  }
}
