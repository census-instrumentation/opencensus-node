{
  "name": "@opencensus/resource-util",
  "version": "0.0.9",
  "description": "The OpenCensus Monitored Resource Util for Node.js is a collection of utilities for auto detecting monitored resource when exporting stats, based on the environment where the application is running.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "census-instrumentation/opencensus-node",
  "scripts": {
    "test": "nyc mocha build/test/**/*.js",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "clean": "rimraf build/*",
    "check": "gts check",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "opencensus",
    "nodejs",
    "stats",
    "resource"
  ],
  "author": "OpenCensus Authors",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.4.7",
    "codecov": "^3.1.0",
    "gts": "^0.9.0",
    "mocha": "^6.0.0",
    "nock": "^10.0.0",
    "nyc": "^13.0.0",
    "rimraf": "^2.6.2",
    "typescript": "~3.2.0"
  },
  "dependencies": {
    "@opencensus/core": "^0.0.9",
    "gcp-metadata": "^1.0.0"
  }
}
